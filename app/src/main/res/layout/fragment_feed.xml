<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.main.mypage.profilemodify.ProfileEditActivity"
    android:background="#ffffff">

    <RelativeLayout
        android:id="@+id/rl_search_map"
        android:layout_width="329dp"
        android:layout_height="45dp"
        android:layout_marginTop="16dp"
        android:layout_gravity="center"
        android:background="@drawable/searchbar_border_background"
        >
        <EditText
            android:layout_width="280dp"
            android:layout_height="match_parent"
            android:paddingStart="20dp"
            android:paddingVertical="9dp"
            android:singleLine="true"
            android:textColor="#000000"
            android:textColorHint="#c5c5c5"
            android:background="@color/transparent"
            android:textSize="16sp"
            />
        <ImageButton
            android:id="@+id/btn_search_feed"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:background="@drawable/btn_search"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="16dp"
            android:layout_centerVertical="true"
            />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#eeeeee"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="15dp"
        />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_feed_content_feed"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
</LinearLayout>